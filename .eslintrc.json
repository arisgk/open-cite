{
  "parser": "babel-eslint",
  "plugins": ["react", "@typescript-eslint", "office-addins", "prettier"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "project": "./tsconfig.json"
  },
  "extends": [
    "airbnb",
    "plugin:office-addins/react",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "react/prop-types": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
